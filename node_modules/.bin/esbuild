#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules/esbuild/bin/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules/esbuild/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/node_modules:/Users/aacgectyuoki/DJEI/node_modules:/Users/aacgectyuoki/node_modules:/Users/node_modules:/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules/esbuild/bin/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules/esbuild/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/node_modules:/Users/aacgectyuoki/DJEI/node_modules:/Users/aacgectyuoki/node_modules:/Users/node_modules:/node_modules:/Users/aacgectyuoki/DJEI/djei-full-stack/djei-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
